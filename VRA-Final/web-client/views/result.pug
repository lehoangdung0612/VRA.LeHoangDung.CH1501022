extend layout

block content
    
    #wrapper
        .container
            if data.length > 0
                h4 Showing results of #{data.length} images
                div#img-container
                    ol.list(type="1")
                        each item in data
                            li.image.loading
                                a.link(href= item.image, target="_blank")
                                    img.b-lazy(src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==", data-src= item.image)
                                div.score Score = #{item.score}
            else
                h4 No images are found